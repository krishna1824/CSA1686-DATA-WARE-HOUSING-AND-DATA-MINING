age<-c(23,23,27,27,39,41,47,49,50,52,54,54,56,57,58,58,60,61)
pern<-c(9.5,26.5,7.8,17.8,31.4,25.9,27.4,27.2,31.2,34.6,42.5,28.8,33.4)
age
pern

df1<-data.frame(age,pern)
df1
#a
mean(df1$age)
mean(df1$pern)
median(df1$age)
median(df1$pern)
#b

boxplot(age)
boxplot(pern)

#c
qqplot(age, pern, xlab="Theoretical Quantiles", ylab="Sample Quantiles", main="Age vs. %Fat Q-Q Plot")
abline(a=0, b=1) 
