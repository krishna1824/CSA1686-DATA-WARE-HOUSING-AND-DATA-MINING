x<-data.frame(Name=c("PRANEETH","VANAJA","STEVE","JAYAKRISHNA"),
                     Age=c(20,22,14,6),
              Gender=c("male","female","male","male"),
              Studies=c("scl","clg","clg","scl"),
              Marks=c(89,76,92,56),
              Grade=c("B","C","S","A"),
              stringsAsFactors = FALSE
                     )
View(x)
write.csv(x,"x.csv")
